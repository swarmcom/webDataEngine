{
  "identification_schema": {
    "type": "object",
    "title": "Identification",
    "properties": {
      "id": {
        "type": "string",
        "title": "Id"
      },
      "providerName": {
        "type": "string",
        "title": "Provider name"
      },
      "superadminUserName": {
        "type": "string",
        "title": "Superadmin User Name"
      },
      "superadminPassword": {
        "type": "string",
        "title": "Superadmin Initial Password",
        "format": "password",
        "required": true
      },
      "digestEncoded": {
        "type": "boolean",
        "title": "Digest"
      },
      "description": {
        "type": "string",
        "title": "Description"
      },
      "email": {
        "type": "string",
        "title": "Email",
        "format": "email"
      }
    }
  },
  "identification_options": {
    "renderForm":true,
    "fields": {
      "id": {
        "type": "hidden"
      },
      "superadminUserName": {
        "helper": "Please enter superadmin user name"
      },
      "superadminPassword": {
        "helper": "Please enter superadmin password"
      },
      "digestEncoded": {
        "helper": "If checked, the password will be encoded in digest format"
      },
      "description": {
        "type": "textarea",
        "helper": "Please enter provider description"
      },
      "email": {
        "helper": "Please enter provider email"
      }
    }
  },
  "settings_defaults": {
    "providerName": "none"
  }
}